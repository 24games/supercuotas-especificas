{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(html|png|jpg|jpeg|gif|svg|css|js|txt))",
      "dest": "/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    }
  ]
}

